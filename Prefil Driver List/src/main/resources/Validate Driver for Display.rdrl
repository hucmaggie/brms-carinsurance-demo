import redhat.insurance.prefildriverlist.Driver;
import java.lang.Number;

rule "Validate Driver for Display"
	dialect "mvel"
	no-loop true
	ruleflow-group "validateDriver"
	when
		$driver : Driver( dateOfBirth == null && ( dateOfBirth != null && age >= 15 ))
	then
		modify( $driver ) {
				setDisplayed( true )
		}
end
